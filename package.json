{
  "name": "number-pairs",
  "version": "1.0.0-dev.4",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/number-pairs.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "NUMBER_PAIRS",
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "supportsOutputJS": true,
    "simFeatures": {
      "supportsDynamicLocale": true,
      "supportsSound": true,
      "supportsInteractiveDescription": true
    },
    "screenNameKeys": [
      "NUMBER_PAIRS/screen.intro",
      "NUMBER_PAIRS/screen.ten",
      "NUMBER_PAIRS/screen.twenty",
      "NUMBER_PAIRS/screen.sum",
      "NUMBER_PAIRS/screen.game"
    ]
  }
}