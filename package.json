{
  "name": "number-pairs",
  "version": "1.1.0-dev.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/number-pairs.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3",
    "@playwright/test": "^1.40.0"
  },
  "scripts": {
    "test": "playwright test"
  },
  "phet": {
    "requirejsNamespace": "NUMBER_PAIRS",
    "phetLibs": [
      "counting-common",
      "number-suite-common",
      "vegas"
    ],
    "simulation": true,
