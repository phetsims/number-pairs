      "adapted-from-phet"
    ],
    "supportsOutputJS": true,
    "simFeatures": {
      "supportsDynamicLocale": true,
      "supportsSound": true,
      "supportsInteractiveDescription": true
    },
    "screenNameKeys": [
      "NUMBER_PAIRS/screen.intro",
      "NUMBER_PAIRS/screen.ten",
      "NUMBER_PAIRS/screen.twenty",
      "NUMBER_PAIRS/screen.sum",
      "NUMBER_PAIRS/screen.game"
    ],
    "published": true
